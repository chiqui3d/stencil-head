{
  "name": "stencil-head",
  "version": "1.1.0",
  "description": "Head component for Stencil and Web Components",
  "homepage": "https://github.com/chiqui3d/stencil-head",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chiqui3d/stencil-head.git"
  },
  "keywords": [
    "Stencil",
    "StencilJS",
    "Stencil head",
    "Stencil meta tags",
    "Stencil head component",
    "Head web component",
    "Meta tags component",
    "Seo component"
  ],
  "author": "chiqui3d",
  "main": "dist/index.js",
